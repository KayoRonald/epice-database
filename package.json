{
  "name": "epice-database",
  "version": "1.0.0",
  "description": "Back-end do Epice",
  "main": "src/server.ts",
  "scripts": {
    "start": "tsc && node ./dist/src/server.js",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "knex:migrate:make": "knex --knexfile src/models/knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "knex --knexfile src/models/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/models/knexfile.ts migrate:rollback",
    "test": "jest"
  },
  "keywords": [],
  "author": "KayoRoanld",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-async-errors": "^3.1.1",
    "knex": "^2.0.0",
    "mysql": "^2.18.1",
    "sqlite3": "^5.0.8",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.29",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}

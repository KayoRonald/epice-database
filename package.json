{
  "name": "epice-database",
  "version": "1.0.0",
  "description": "Back-end do Epice",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/src/server.js",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "migrate:latest": "cross-env NODE_ENV=production knex --knexfile src/models/knexfile.ts migrate:latest",
    "migrate:rollback": "cross-env NODE_ENV=production knex --knexfile src/models/knexfile.ts migrate:rollback"
  },
  "keywords": [
    "crud",
    "Knexjs",
    "epice"
  ],
  "author": "KayoRoanld",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.1",
    "knex": "^2.1.0",
    "mysql": "^2.18.1",
    "pg": "^8.7.3",
    "sqlite3": "^5.0.8",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.41",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.2",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
